{
    "name": "eloquent-coder/log-sender",
    "description": "A log sender that sends logs to the LogMonitor application",
    "keywords": ["logging", "monitoring", "laravel"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Obafemi",
            "email": "eloquentintech@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "EloquentCoder\\LogSender\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EloquentCoder\\LogSender\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EloquentCoder\\LogSender\\LogSenderServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
